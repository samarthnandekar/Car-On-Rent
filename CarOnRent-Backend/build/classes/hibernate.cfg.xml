<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/caronrent</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.connection.password"></property>
		
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
		
		<property name="hibernate.show_sql">true</property>
				  
		
		<mapping class="com.CarOnRent.myapp.pojo.AccountDetails"/>
		<mapping class="com.CarOnRent.myapp.pojo.Address"/>
		
		 
		 <mapping class="com.CarOnRent.myapp.pojo.AllMasterAvailability"/>
		 <mapping class="com.CarOnRent.myapp.pojo.Availability"/>
		 <mapping class="com.CarOnRent.myapp.pojo.CarDetails"/>
		 <mapping class="com.CarOnRent.myapp.pojo.CarInsurance"/>
		 <mapping class="com.CarOnRent.myapp.pojo.Carmaker"/>
		 
		 <mapping class="com.CarOnRent.myapp.pojo.CarParkingaddress"/>
		 
		  <mapping class="com.CarOnRent.myapp.pojo.UserAccount"/>
		 <mapping class="com.CarOnRent.myapp.pojo.UserCar"/>
		 <mapping class="com.CarOnRent.myapp.pojo.UsercarReservation"/>
		 
		 <mapping class="com.CarOnRent.myapp.pojo.CarModel"/>
		 
		 <mapping class="com.CarOnRent.myapp.pojo.PaymentDetails"/>
		 
		 
		  
						 
	</session-factory>
</hibernate-configuration>
